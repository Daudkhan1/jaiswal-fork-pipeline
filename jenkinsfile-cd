pipeline {
    agent any 

    stages {
        stage('Docker Deploy To Container') {
            steps {
                withDockerRegistry(credentialsId: 'dockerhub-id', url: 'https://index.docker.io') {
                    sh 'docker run -d -p 8070:8070 daudidrees/shopping-cart:latest'
                }
            }
        }
    }
}
